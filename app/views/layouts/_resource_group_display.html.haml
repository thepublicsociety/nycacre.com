- @results.each do |speciality, providers|
  %div{:class => "#{speciality} provider_group resource_group"}
    %h3
      = speciality
    %ul.unstyled
      - providers.each do |p|
        %li
          - if p.saved?(@user)
            .saved_indicator
          .name
            = link_to p.name, "#", 'data-resource_type' => 'provider', 'data-resource_id' => p.id, :class => "resource_link"