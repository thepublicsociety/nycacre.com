<h1>Listing funding_status_options</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @funding_status_options.each do |funding_status_option| %>
  <tr>
    <td><%= funding_status_option.name %></td>
    <td><%= funding_status_option.value %></td>
    <td><%= link_to 'Show', funding_status_option %></td>
    <td><%= link_to 'Edit', edit_funding_status_option_path(funding_status_option) %></td>
    <td><%= link_to 'Destroy', funding_status_option, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

