<%= render :partial => "layouts/cms_header" %>
<div class="content">
<h1>Listing applicants</h1>

<table class="table">
  <tr>
    <th>Company</th>
    <th>Name and Title</th>
    <th>Address</th>
    <th>Phone</th>
    <th>Email</th>
    <th>Cash Flow Statement</th>
    <th>Current Structure File</th>
    <th></th>
    <th></th>
  </tr>

<% @applicants.each do |a| %>
  <tr>
    <td><%= a.company_name %></td>
    <td><%= a.name_and_title %></td>
    <td><%= a.address %></td>
    <td><%= a.phone %></td>
    <td><%= a.email %></td>
    <td><%= link_to a.cash_flow_statement_file_name, a.cash_flow_statement.url %></td>
    <td><%= link_to a.current_structure_file_file_name, a.current_structure_file.url %></td>
    <td><%= link_to 'Show', a %></td>
    <td><%= link_to 'Destroy', a, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

</div>