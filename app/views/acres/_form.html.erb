<%= simple_form_for(@acre) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :subtitle %>
    <%= f.input :content %>
    <%= f.input :acre_image %>
    <%= f.input :featured %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
